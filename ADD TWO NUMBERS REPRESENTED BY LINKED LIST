class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None
l1 = LinkedList()
l1.head = Node(2)
l1.head.next = Node(4)
l1.head.next.next = Node(3)


l2 = LinkedList()
l2.head = Node(5)
l2.head.next = Node(6)
l2.head.next.next = Node(4)


p1 = l1.head
p2 = l2.head
carry = 0

result = LinkedList()
result_head = None
result_tail = None

while p1 or p2 or carry:
    val1 = p1.data if p1 else 0
    val2 = p2.data if p2 else 0
    total = val1 + val2 + carry
    carry = total // 10
    digit = total % 10

    new_node = Node(digit)
    if result_head is None:
        result_head = new_node
        result_tail = new_node
    else:
        result_tail.next = new_node
        result_tail = new_node

    p1 = p1.next if p1 else None
    p2 = p2.next if p2 else None

result.head = result_head


temp = result.head
while temp:
    print(temp.data, end=" -> ")
    temp = temp.next
print("None")
