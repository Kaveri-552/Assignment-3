class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None


ll = LinkedList()
ll.head = Node(10)
second = Node(20)
third = Node(30)

ll.head.next = second
second.next = third


key = 20
curr = ll.head
prev = None

while curr and curr.data != key:
    prev = curr
    curr = curr.next

if curr:  
    if prev:
        prev.next = curr.next
    else: 
        ll.head = curr.next


temp = ll.head
while temp:
    print(temp.data, end=" -> ")
    temp = temp.next
print("None")
