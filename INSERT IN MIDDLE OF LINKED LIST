class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None


ll = LinkedList()
ll.head = Node(1)
second = Node(2)
third = Node(4)


ll.head.next = second
second.next = third


slow = ll.head
fast = ll.head
prev = None

while fast and fast.next:
    prev = slow
    slow = slow.next
    fast = fast.next.next


new_node = Node(3)
prev.next = new_node
new_node.next = slow


temp = ll.head
while temp:
    print(temp.data, end=" -> ")
    temp = temp.next
print("None")
