class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None


ll = LinkedList()
ll.head = Node(11)
ll.head.next = Node(22)
ll.head.next.next = Node(33)
ll.head.next.next.next = Node(44)
key = 33
temp = ll.head
pos = 0
found = False

while temp:
    if temp.data == key:
        print(f"Element {key} found at position {pos}")
        found = True
        break
    temp = temp.next
    pos += 1

if not found:
    print(f"Element {key} not found")
